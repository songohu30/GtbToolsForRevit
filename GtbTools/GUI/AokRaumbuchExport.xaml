<Window x:Class="GUI.AokRaumbuchExport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="AokRaumbuchExport" Height="250" Width="300"
        Background="Gray"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize">
    <Grid Background="White" Margin="5">
        <Button x:Name="ExportButton" Content="MEP Räume analysieren" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Width="200" Click="ExportButton_Click"/>
        <Button x:Name="SaveLogButton" IsEnabled="False" Content="Detailed Extract Speichern als..." HorizontalAlignment="Center" Margin="0,110,0,0" VerticalAlignment="Top" Width="200" Click="SaveLogButton_Click"/>
        <Button x:Name="ExportToExcel" Content="Export to Excel" IsEnabled="False" HorizontalAlignment="Center" Margin="0,140,0,0" VerticalAlignment="Top" Width="200" Click="ExportToExcel_Click"/>
    </Grid>
</Window>
